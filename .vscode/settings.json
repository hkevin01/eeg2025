{
  "files.watcherExclude": {
    "**/logs/**": true,
    "**/experiments/**": true,
    "**/checkpoints/**": true,
    "**/data/**": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.hg/store/**": true,
    "**/venv*/**": true,
    "**/__pycache__/**": true,
    "**/archive/**": true,
    "**/old_*/**": true
  },
  "files.exclude": {
    "**/__pycache__": true,
    "**/*.pyc": true
  },
  "search.exclude": {
    "**/venv*": true,
    "**/data": true,
    "**/logs": true,
    "**/experiments": true,
    "**/checkpoints": true,
    "**/archive": true
  },
  "extensions.autoUpdate": false,
  "extensions.autoCheckUpdates": false,
  "python.linting.enabled": false,
  "python.formatting.provider": "none",
  "files.watcherMaxFolderSize": 512000,
  "python.analysis.diagnosticMode": "openFilesOnly",
  "python.analysis.typeCheckingMode": "off"
}
