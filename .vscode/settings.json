{
  "files.watcherExclude": {
    "**/logs/**": true,
    "**/data/cached/**": true,
    "**/checkpoints/**": true,
    "**/data/**": true,
    "**/__pycache__/**": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true,
    "**/archive/**": true
  },
  "files.exclude": {
    "**/*.pyc": true,
    "**/__pycache__": true
  },
  "search.exclude": {
    "**/logs/**": true,
    "**/data/cached/**": true,
    "**/archive/**": true,
    "**/__pycache__": true
  },
  "search.followSymlinks": false,
  "search.useIgnoreFiles": true,
  "files.maxMemoryForLargeFilesMB": 4096,
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "files.associations": {
    "*.log": "plaintext"
  },
  "intellicode.enable.python": false,
  "intellicode.enable.general": false,
  "makefile.configureOnOpen": false
}
